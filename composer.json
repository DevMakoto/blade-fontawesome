{
    "name": "owenvoke/blade-fontawesome",
    "type": "library",
    "description": "A package to easily make use of Font Awesome in your Laravel Blade views",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "blade-ui-kit/blade-icons": "^0.4.3",
        "illuminate/support": "^7.14"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "pestphp/pest": "^0.2",
        "phpstan/phpstan": "^0.12.31",
        "rector/rector": "^0.7.48",
        "spatie/pest-plugin-snapshots": "^0.2.0",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\BladeFontAwesome\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\BladeFontAwesome\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "rector process src",
        "test:lint": "rector process src --dry-run",
        "test:unit": "pest",
        "test": [
            "@test:lint",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwenVoke\\BladeFontAwesome\\BladeFontAwesomeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
